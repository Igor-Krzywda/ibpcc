INT = 100
FLOAT = 3.14
STRING = "Hello world!"

ARRAY = [1,2,3,4,5]
ARRAY_2D = [[1,1],[2,2],[3,3]]
ARRAY_3D = [[[1,1],[1,1]],[[2,2],[2,2]]]

ARRAY_EMPTY = Array(10)

STACK = Stack()
QUEUE = Queue()

loop I from 0 to 3
    STACK.push(I)
    QUEUE.enqueue(I)
end loop

output("This is an INT: ", INT)
output("This is a FLOAT: ", FLOAT)
output("This is a STRING: ", STRING)

output("This is an ARRAY: ", ARRAY)

output("This is a STACK: ")
loop while STACK.isEmpty() != 1
    output(STACK.pop())
end loop

output("This is a QUEUE: ")
loop while QUEUE.isEmpty() != 1
    output(QUEUE.dequeue())
end loop

output("Hello, world! My name is " + input("Concatenating strings (enter your name): "))

